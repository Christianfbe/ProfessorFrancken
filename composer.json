{
  "name": "professorfrancken/professorfrancken",
  "license": "MIT",
  "type": "project",
  "require": {
    "blade-ui-kit/blade-icons": "^0.4.2",
    "cmpayments/iban": "^1.1",
    "doctrine/dbal": "^2.10",
    "facade/ignition": "^2.0",
    "fideloper/proxy": "^4.4",
    "guzzlehttp/guzzle": "^6.5",
    "intervention/validation": "^2.4",
    "laravel/framework": "^7.0",
    "laravel/helpers": "^1.2",
    "laravel/horizon": "^4.3",
    "laravel/telescope": "^3.5",
    "laravel/tinker": "^2.4",
    "laravel/ui": "^2.0",
    "laravelcollective/html": "^6.1",
    "lcobucci/jwt": "^3.2",
    "league/commonmark": "^1.5",
    "league/html-to-markdown": "^4.9",
    "league/period": "^4.10",
    "maatwebsite/excel": "^3.1",
    "plank/laravel-mediable": "^4.2",
    "predis/predis": "^1.1",
    "ramsey/uuid": "^3.0",
    "sabre/vobject": "^4.1",
    "spatie/laravel-permission": "^3.13",
    "spatie/valuestore": "^1.2",
    "webmozart/assert": "^1.9",
    "webuni/commonmark-attributes-extension": "^1.0"
  },
  "require-dev": {
    "barryvdh/laravel-debugbar": "^3.3",
    "barryvdh/laravel-ide-helper": "^2.7",
    "beyondcode/laravel-dump-server": "^1.4",
    "codedungeon/phpunit-result-printer": "0.27.*",
    "friendsofphp/php-cs-fixer": "^2.10",
    "fzaninotto/faker": "~1.4",
    "laravel/browser-kit-testing": "^6.0",
    "laravel/envoy": "^2.1",
    "mockery/mockery": "^1.0",
    "nunomaduro/collision": "^4.0",
    "nunomaduro/larastan": "^0.6.1",
    "php-coveralls/php-coveralls": "^2.0",
    "phpstan/phpstan": "^0.12.30",
    "phpunit/phpunit": "~8.0",
    "psalm/plugin-laravel": "^1.3.0",
    "rector/rector": "^0.7.37",
    "roave/security-advisories": "dev-master",
    "vimeo/psalm": "^3.12"
  },
  "autoload": {
    "files": ["src/helpers.php"],
    "classmap": ["database"],
    "psr-4": {
      "Francken\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Francken\\Features\\": "tests/Features",
      "Francken\\Tests\\": "tests/Francken"
    }
  },
  "extra": {
    "laravel": {
      "dont-discover": [
        "laravel/nexmo-notification-channel",
        "laravel/slack-notification-channel"
      ]
    }
  },
  "scripts": {
    "post-root-package-install": ["php -r \"copy('.env.example', '.env');\""],
    "post-create-project-cmd": ["@php artisan key:generate --ansi"],
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi"
    ],
    "units": "vendor/bin/phpunit --testsuite 'Unit Test Suite'",
    "features": "vendor/bin/phpunit --testsuite 'Application Test Suite'",
    "cs": "vendor/bin/php-cs-fixer fix --dry-run",
    "phpstan": "vendor/bin/phpstan analyze src",
    "psalm": "vendor/bin/psalm",
    "test": "composer run cs && composer run units && composer run features"
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
