{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.0.0",
        "laravel/framework": "5.3.*",
        "broadway/broadway": "^0.9.0",
        "webmozart/assert": "^1.1",
        "matthiasnoback/broadway-serialization": "^1.0",
        "laravelcollective/html": "^5.2",
        "mattketmo/uuid-2x-bridge": "*@dev",
        "ramsey/uuid": "^3.0",
        "league/commonmark": "^0.15.0"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "humbug/humbug": "dev-master",
        "phpunit/phpunit": "~5.2",
        "symfony/css-selector": "2.8.*|3.0.*",
        "symfony/dom-crawler": "2.8.*|3.0.*",
        "fabpot/php-cs-fixer": "^1.11",
        "satooshi/php-coveralls": "^1.0",
        "davidbadura/faker-markdown-generator": "^1.0"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
          "Francken\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Francken\\Features\\": "tests/Features",
            "Francken\\Tests\\": "tests/Francken"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "pre-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan clear-compiled"
        ],
        "post-update-cmd": [
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "dev",
    "prefer-stable" : true
}
